        <script>document.addEventListener('DOMContentLoaded', () => {
                const closeButton = document.getElementById('closeDrawerButton');
                const drawerToggle = document.getElementById('drawer-toggle');

                // Event listener for the close button
                closeButton.addEventListener('click', () => {
                    drawerToggle.checked = false; // Uncheck the drawer toggle checkbox
                });
                const serviceButton = document.getElementById('service-dropdown');
                const serviceDropdownOpen = document.getElementById('service-dropdown-open');

                serviceDropdownOpen.addEventListener('click', () => {
                    const isHidden = serviceButton.classList.contains('hidden');

                    if (isHidden) {
                        // Element is currently hidden, so show it
                        serviceButton.classList.remove('hidden');
                    } else {
                        // Element is currently visible, so hide it
                        serviceButton.classList.add('hidden');
                    }

                })

            });
        </script>
