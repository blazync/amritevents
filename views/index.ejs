                                    </ul>
                                    <h3 class="blog_title"><a href="/blog?slug=<%- data.slug %>">
                                            <%= data.title %>
                                        </a></h3>
                                    <a href="/blog?slug=<%- data.slug %>" class="more_btn">Read More</a>
                                </div>
                            </div> <!-- row -->
                        </div>
                        <% }) %>
                </div> <!-- row -->
            </div> <!-- container -->
        </section>


