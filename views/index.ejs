                </div>

                <div class='grid grid-cols-1 md:grid-cols-2 px-10 py-10 mt-8 gap-10'>
                    <div class=' text-center justify-center items-center'>
                        <img src="../img/amrit light show.jpg" alt="" class="w-full rounded-lg" data-aos="fade-right" />
                        <br />
                        <span class='flex justify-center items-center'>
                            <img src='../img/logo.png' alt="" class="h-20 w-60" />
                        </span>
                    </div>
                    <div class='px-2 text-2xl text-justify' data-aos="fade-left">Amrit Creation is a full-service event
                        and wedding
                        planning company
                        located in
                        C-127, Harmu Housing Colony,Harmu, Ranchi, Jharkhand <br />

                        <br /> We offer professional event planning and co-ordination service for intimate corporate
                        event, weddings and parties & more....

                        <br />

                        <br /> we are dedicated to transforming your corporate events into unforgettable experiences
                        that inspire, celebrate
                        achievements, and foster meaningful connections. Based in Jharkhand, we bring a wealth of
                        knowledge, creativity, and
                        professionalism to the event management industry, ensuring that each event is not just executed,
                        but celebrated.<br />

                        <br /> Explore our comprehensive range of event management services tailored to elevate your
                        brand and make every event a
                        landmark.
                        <br />

                        <div class=" text-red-400">
                            <a href="/contact">Read More...</a>
                        </div>

                        <!-- <button
                            class='mt-8 border border-orange-500 rounded-md px-2 py-2 text-orange-700 hover:bg-orange-500 hover:text-white'>
                            <Link to="/about">
                            <h1>Read More</h1>
                            </Link>

                        </button> -->

                    </div>



                </div>

            </div>
        </section>
        <!-- About Information End -->




        <!--====== SERVICES PART START ======-->
        <hr>
        <section class="services_area pt-10 animate-slide-in-bottom" id="about">
            <div class="container">
                <div class="row justify-center">
                    <div class="w-full lg:w-1/2">
                        <div class="section_title text-center pb-6">
                            <h5 class="sub_title">Services</h5>
                            <h4 class="main_title">Our Main Service Category</h4>
                        </div> <!-- section title -->
                    </div>
                </div> <!-- row -->
                <div class="row justify-center" data-aos="fade-up">
                    <% category.forEach(data=> { %>
                        <div class="w-full sm:w-10/12 md:w-6/12 lg:w-4/12">
                            <a href="/services/<%= data.slug %>">
                                <div class="single_services text-center mt-8 mx-1">
                                    <div class="services_icon">
                                        <i class="lni lni-write"></i>
                                        <img src="img/event icon 1.jpg" alt="" style="height: 94;">
                                    </div>
                                    <div class="services_content mt-5">
                                        <h3 class="services_title text-black font-semibold text-xl md:text-3xl">
                                            <%= data.title %>
                                        </h3>
                                        <div class="flex justify-center items-center">
                                            <h5
                                                class="mt-4 text-xl text-justify text-bold text-black text-center mx-auto">
                                                <%= data.shortdescription %>
                                            </h5>
                                        </div>
                                        <p class="mt-4 text-xl text-justify">
                                            <%= data.description %>
                                        </p>
                                    </div>
                                </div> <!-- single services -->
                            </a>
                        </div>
                        <% }) %>

                </div> <!-- row -->
            </div> <!-- container -->
        </section>

        <!--====== SERVICES PART ENDS ======-->

        <!-- Services Under Corporate Events -->
        <section>
            <div class="pt-20  lg:pt-[60px]">
                <h1 class=" px-10 font-bold bg-[#2dd4bf] py-5
                                                      text-2xl
                                                        text-black mb-2 block">
                    Services Under Corporate Events
                </h1>
                <aside aria-label="Related articles" class="py-8 lg:py-24 bg-gray-100  ">
                    <div class="px-4 mx-auto max-w-screen-xl ">
                        <div class="grid gap-12 sm:grid-cols-2 lg:grid-cols-3 ">
                            <% category.forEach(categoryData=> { %>
                                <% if (categoryData.title==='Corporate Events' ) { %>
                                    <% if (categoryData.subcategory && categoryData.subcategory.length> 0) { %>
                                        <% categoryData.subcategory.forEach(subcategoryData=> { %>
                                            <article class="max-w-xl" data-aos="zoom-in-up">
                                                <a href="/product/<%= subcategoryData.slug %>">
                                                    <img src="../uploads/<%= subcategoryData.imageUrl %>"
                                                        class="mb-5 rounded-lg w-full h-[300px] " alt="Image 1">
                                                </a>
                                                <h2
                                                    class="mb-2 text-xl font-bold leading-tight text-gray-900 dark:text-white">
                                                    <a href="/product/<%= subcategoryData.slug %>">
                                                        <%= subcategoryData.name %>
                                                    </a>
                                                </h2>
                                                <p class="mb-4 text-gray-500 dark:text-gray-400">
                                                    <%= subcategoryData.shortdescription %>
                                                </p>
                                                <a href="/product/<%= subcategoryData.slug %>"
                                                    class="inline-flex items-center font-medium underline underline-offset-4 text-primary-600 dark:text-primary-500 hover:no-underline">
                                                    Read in 2 minutes
                                                </a>
                                            </article>
                                            <% }) %>
                                                <% } %>
                                                    <% } %>
                                                        <% }) %>



                        </div>
                    </div>
                </aside>
            </div>
        </section>
        <!-- Services Under Corporate Events -->


        <!-- Services Main Section End -->

        <!-- Gallary Shortcut -->

        <!-- Main Content of the Gallary page -->
        <section>
            <h2 class="text-2xl font-bold text-gray-800 mb-4 bg-[#2dd4bf] py-4 px-10">Image Gallery</h2>
            <div class="container mx-auto px-4 py-8">


                <!-- Image Gallery -->
                <div class="grid grid-cols-2 sm:grid-cols-2 md:grid-cols-3 gap-4">
                    <!-- Image 1 -->
                    <% gallery.forEach(data=> { %>
                        <% if(data.filetype=='image' ) { %>
                            <div class="bg-white justify-center rounded-lg  shadow-md w-auto" data-aos="flip-up">
                                <img src="../uploads/<%= data.imageUrl %>" alt="Image"
                                    class="w-full h-[300px] rounded-lg" style="100%">
                            </div>
                            <% } %>

                                <% }); %>


                                    <!-- Image 4 -->
                </div>
                <div class="flex justify-center items-center">
                    <a href="/gallary"><button class="mx-auto py-2 px-5 bg-red-500 text-black">View More</button></a>
                </div>
            </div>
            <h2 class="text-2xl font-bold text-gray-800 mb-4 bg-[#2dd4bf] py-4 px-10">Video Gallery</h2>

            <!-- Video Gallery -->
            <div class="container mx-auto px-4 py-8">
                <div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 gap-4 ">
                    <% gallery.forEach(data=> { %>
                        <% if(data.filetype=='video' ) { %>
                            <div class="responsive-container">
                                <iframe src="https://www.youtube.com/embed/<%= data.imageUrl %>" data-aos="flip-down"
                                    frameborder="0" allowfullscreen class="w-full h-[300px] rounded-lg"></iframe>
                            </div>
                            <% } %>
                                <% }); %>
                </div>
            </div>
            <div class="flex justify-center items-center">
                <a href="/gallary"><button class="mx-auto py-2 px-5 bg-red-500 text-black">View More</button></a>
            </div>
            <!-- <button>View More</button> -->
        </section>

        <!-- Galarray Shortcut -->








